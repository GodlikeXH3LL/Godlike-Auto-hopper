-- Functionality

local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")
local HttpService = game:GetService("HttpService")

local player = Players.LocalPlayer
local PLACE_ID = 3623096087

local isRunning = false

local function findSuitableServer()
	local cursor = ""
	local bestServer = nil
	local bestCount = math.huge

	repeat
		local url = string.format(
			"https://games.roblox.com/v1/games/%d/servers/Public?sortOrder=Asc&limit=100%s",
			PLACE_ID,
			cursor ~= "" and "&cursor=" .. cursor or ""
		)

		local success, response = pcall(function()
			return HttpService:GetAsync(url)
		end)

		if not success then
			warn("HTTP request failed:", response)
			return nil
		end

		local data = HttpService:JSONDecode(response)

		for _, server in ipairs(data.data or {}) do
			local playerCount = server.playing

			if playerCount >= 17 and playerCount <= 19 and server.id ~= game.JobId then
				return server.id
			end

			if playerCount < bestCount and server.id ~= game.JobId and playerCount > 0 then
				bestCount = playerCount
				bestServer = server.id
			end
		end

		cursor = data.nextPageCursor or ""
	until cursor == ""

	return bestServer
end

local function hopToServer()
	if isRunning then return end
	isRunning = true

	local jobId = findSuitableServer()

	if jobId then
		TeleportService:TeleportToPlaceInstance(PLACE_ID, jobId, player)
	else
		TeleportService:Teleport(PLACE_ID, player)
	end
end

AutoKill.MouseButton1Click:Connect(function()
	if isRunning then
		isRunning = false
		AutoKill.Text = "UNACTIVE"
		AutoKill.TextColor3 = Color3.fromRGB(255, 0, 0)
	else
		isRunning = true
		AutoKill.Text = "ACTIVE"
		AutoKill.TextColor3 = Color3.fromRGB(0, 255, 0)
		hopToServer()
		spawn(function()
			while isRunning do
				task.wait(60)
				if isRunning then
					hopToServer()
				end
			end
		end)
	end
end)

TextButton_2.MouseButton1Click:Connect(function()
	ScreenGui:Destroy()
end)
