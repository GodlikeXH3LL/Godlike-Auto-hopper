AutoKill.Parent = ScreenGui
AutoKill.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
AutoKill.BackgroundTransparency = 1.000
AutoKill.BorderColor3 = Color3.fromRGB(0, 0, 0)
AutoKill.BorderSizePixel = 0
AutoKill.Position = UDim2.new(0.517072201, 0, 0.29640919, 0)
AutoKill.Size = UDim2.new(0, 200, 0, 50)
AutoKill.Font = Enum.Font.FredokaOne
AutoKill.Text = "UNACTIVE"
AutoKill.TextColor3 = Color3.fromRGB(255, 0, 0)
AutoKill.TextSize = 26.000

AutoKill.MouseButton1Click:Connect(function()
	AutoKill.Text = "ACTIVE"
	AutoKill.TextColor3 = Color3.fromRGB(0, 255, 0)

	local HttpService = game:GetService("HttpService")
	local TeleportService = game:GetService("TeleportService")
	local Players = game:GetService("Players")

	local placeId = game.PlaceId
	local currentJobId = game.JobId
	local player = Players.LocalPlayer

	local function findServerWithPlayerCount(minPlayers, maxPlayers)
		local cursor = nil
		repeat
			local url = string.format(
				"https://games.roblox.com/v1/games/%d/servers/Public?sortOrder=Asc&limit=100",
				placeId
			)
			if cursor then
				url = url .. "&cursor=" .. cursor
			end
			
			local success, response = pcall(function()
				return HttpService:GetAsync(url)
			end)
			if not success then
				warn("Failed to fetch servers: " .. response)
				return nil
			end
			
			local data = HttpService:JSONDecode(response)
			
			for _, server in ipairs(data.data) do
				if server.playing >= minPlayers and 
				   server.playing <= maxPlayers and 
				   server.id ~= currentJobId then
					return server.id
				end
			end
			
			cursor = data.nextPageCursor
		until not cursor
		
		return nil
	end

	local targetServerId = findServerWithPlayerCount(15, 18)

	if targetServerId then
		print("Found a server with 15-18 players. Teleporting...")
		TeleportService:TeleportToPlaceInstance(placeId, targetServerId, player)
	else
		print("No server found with 15-18 players.")
	end
end)
